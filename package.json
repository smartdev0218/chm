{
  "name": "chm",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-dev-app": "ng serve --env=dev --proxy-config proxy.conf.json --base-href /chm/",
    "start-app": "ng serve --env=test --proxy-config proxy.conf.json --base-href /chm/",
    "pre-deploy": "rimraf ../src/main/resources/static/*.*",
    "build-app": "ng build --target=production --env=prod --base-href /chm/ --output-hashing none",
    "post-deploy": "xcopy .\\dist\\** ..\\src\\main\\resources\\static",
    "deploy-app": "npm run pre-deploy && npm run build-app && npm run post-deploy",
    "reset": "rimraf package-lock.json",
    "init": "rimraf node_modules && npm install -f",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^5.2.3",
    "@angular/common": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/material": "^5.2.3",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@ngui/datetime-picker": "^0.16.2",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@swimlane/ngx-datatable": "^11.2.0",
    "admin-lte": "2.3.11",
    "angular2-jwt": "^0.2.3",
    "angular2-toaster": "^4.0.1",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "ionicons": "^3.0.0",
    "jquery": "^3.2.1",
    "linqts": "^1.10.1",
    "moment": "^2.24.0",
    "ng2-accordion": "0.0.15",
    "ng2-dnd": "^4.0.0",
    "ngx-bootstrap": "^2.0.2",
    "rxjs": "^5.4.3",
    "simplebar": "^3.1.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.0",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@compodoc/compodoc": "^1.0.1",
    "@types/jquery": "^3.3.1",
    "@types/node": "^8.0.28",
    "codelyzer": "~3.1.2",
    "ts-node": "~3.3.0",
    "tslint": "~5.5.0",
    "typescript": "^2.5.2"
  }
}
